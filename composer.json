{
  "name": "papaya/cms-core",
  "description": "papaya CMS core framework",
  "license": "GPL-2.0",
  "homepage": "http://www.papaya-cms.com/",
  "minimum-stability": "dev",
  "autoload" : {
    "psr-4": {
      "Papaya\\Modules\\Core\\": "src/modules/_base/"
    },
    "files" : ["src/core.php"]
  },
  "require": {
    "php": ">=7.2.0",
    "ext-dom": "*",
    "ext-libxml": "*",
    "ext-xsl": "*",
    "ext-json": "*",
    "ext-gd": "*",
    "psr/log": "1.*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",

    "papaya/cms-administration": "dev-master",
    "papaya/test-framework": "dev-master",

    "papaya/theme-dynamic": "dev-master",

    "papaya/module-default": "dev-master",
    "papaya/module-standard": "dev-master",
    "papaya/module-domains": "dev-master",
    "papaya/module-default-cronjobs": "dev-master"
  }
}
