<field name="mimetype" type="string" size="100"  null="no"/>
<field name="mimetype_icon" type="string" size="100"  null="no"/>
<field name="mimetype_ext" type="string" size="50"  null="no"/>
<field name="mimegroup_id" type="integer" size="4"  null="no"/>
<field name="range_support" type="integer" size="2"  null="no"/>
<field name="shaping" type="integer" size="4"  null="no"/>
<field name="shaping_limit" type="integer" size="4"  null="no"/>
  <field name="shaping_offset" type="integer" size="4"  null="no"/>
  <field name="download_octet_stream" type="integer" size="4"  null="no"/>
</fields>
<keys>
<primary-key>
<field>mimetype_id</field></primary-key>
<key name="mimetype" unique="yes">
<field>mimetype</field></key>
</keys>
</table>
